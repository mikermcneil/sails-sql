{
  "name": "sails-sql",
  "version": "0.1.0-6",
  "description": "Flexible SQL adapter for Sails/Waterline.  Supports MySQL, PostgreSQL, SQL Server (MSSQL), Oracle, & SQLite.",
  "keywords": [
    "adapter",
    "sql",
    "mysql",
    "postgresql",
    "sqlite3",
    "oracle",
    "mssql",
    "sql-server",
    "sails.js",
    "sails",
    "waterline"
  ],
  "main": "lib/sails-sql.js",
  "repository": "git@github.com:mikermcneil/sails-sql.git",
  "driver": {
    "friendlyName": "SQL",
    "implements": {
      "connectable": "1.0",
      "queryable": "1.0",
      "transactional": "1.0",
      "modeled": "1.0",
      "migratable": "1.0"
    }
  },
  "scripts": {
    "custom-tests": "node ./node_modules/mocha/bin/mocha test --recursive -t 10000 -b",
    "lint": "node ./node_modules/eslint/bin/eslint . --max-warnings=0 && echo 'âœ”  Your code looks good.'",
    "bench": "NODE_ENV=production mocha test/*.benchmark.js",
    "test": "npm run lint && npm run custom-tests"
  },
  "author": "Mike McNeil",
  "license": "MIT",
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "@sailshq/lodash": "^3.10.2",
    "driver-interface": "^1.0.5",
    "flaverr": "^1.5.1",
    "knex": "0.16.3",
    "machine": "^15.0.0",
    "mysql": "2.15.0",
    "tedious": "5.0.3",
    "waterline-utils": "^1.4.1"
  },
  "devDependencies": {
    "benchmark": "2.1.2",
    "eslint": "4.19.1",
    "mocha": "5.2.0"
  }
}
